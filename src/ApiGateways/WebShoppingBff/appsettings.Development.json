{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Routes": [
    {
      "DownstreamPathTemplate": "/todos/{id}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "jsonplaceholder.typicode.com",
          "Port": 443
        }
      ],
      "UpstreamPathTemplate": "/todos/{id}",
      "UpstreamHttpMethod": [
        "Get"
      ]
    },
    {
      "DownstreamPathTemplate": "api/v1/catalog/items/{id}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "https://localhost/",
          "Port": 8081
        }
      ],
      "UpstreamPathTemplate": "/catalog/items/{id}",
      "UpstreamHttpMethod": [
        "Get",
        "Delete",
        "Put"
      ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:5192"
  }
}